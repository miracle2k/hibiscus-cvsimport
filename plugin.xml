<?xml version="1.0" encoding="ISO-8859-1"?>

<plugin name="hibiscus" version="1.9" class="de.willuhn.jameica.hbci.HBCI">

  <description>HBCI-Onlinebanking-Plugin für Jameica</description>
  <url>http://www.willuhn.de/projects/hibiscus/releases/hibiscus.jam</url>
  <homepage>http://www.willuhn.de/projects/hibiscus</homepage>
  <license>GPL - http://www.gnu.org/copyleft/gpl.html</license>

  <menu>
    <item id="hibiscus.menu"                         name="Hibiscus">
      <item id="hibiscus.menu.transfer"              name="Zahlungsverkehr">
        <item id="hibiscus.menu.transfer.ueb"        name="Neue Überweisung..."       shortcut="ALT+U"    action="de.willuhn.jameica.hbci.gui.action.UeberweisungNew" />
        <item id="hibiscus.menu.transfer.last"       name="Neue Lastschrift..."       shortcut="ALT+L"    action="de.willuhn.jameica.hbci.gui.action.LastschriftNew" />
        <item id="hibiscus.menu.transfer.sammelueb"  name="Neue Sammelüberweisung..." shortcut="ALT+S"    action="de.willuhn.jameica.hbci.gui.action.SammelUeberweisungNew" />
        <item id="hibiscus.menu.transfer.sammellast" name="Neue Sammellastschrift..." shortcut="ALT+F"    action="de.willuhn.jameica.hbci.gui.action.SammelLastschriftNew" />
      </item>
      <item id="hibiscus.menu.settings"              name="Einstellungen"                            action="de.willuhn.jameica.hbci.gui.action.Settings" />
      <item name="-" />
      <item id="hibiscus.menu.handbuch"              name="Handbuch"                                 action="de.willuhn.jameica.hbci.gui.action.HandbuchAction"/>
      <item id="hibiscus.menu.donate"                name="Spenden"                                  action="de.willuhn.jameica.hbci.gui.action.Donate" />
      <item name="-" />
      <item id="hibiscus.menu.about"                 name="About"                                    action="de.willuhn.jameica.hbci.gui.action.About" />
      <item name="-" />
      <item id="hibiscus.menu.ext"                   name="Erweitert">
        <item id="hibiscus.menu.ext.license"         name="Lizenzinformationen"                      action="de.willuhn.jameica.hbci.gui.action.License" />
        <item name="-" />
        <item id="hibiscus.menu.ext.backup.create"   name="Diagnose-Backup erstellen"                action="de.willuhn.jameica.hbci.gui.action.BackupCreate" />
        <item id="hibiscus.menu.ext.backup.restore"  name="Diagnose-Backup importieren"              action="de.willuhn.jameica.hbci.gui.action.BackupRestore" />
      </item>
    </item>
  </menu>
  
  <classfinder>
    <include>hibiscus\.jar</include>
    <include>hbci_passport_.*\.jar</include>
    <include>.*\.class</include>
    <include>ext_.*\.jar</include>
  </classfinder>

  <navigation>
    <item id="hibiscus.navi"                                name="Hibiscus"             icon-close="folder.gif" icon-open="folderopen.gif">
      <item id="hibiscus.navi.accounts"                     name="Konten"               icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.KontoList" />
      <item id="hibiscus.navi.addresses"                    name="Adressbuch"           icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.EmpfaengerList" />
      <item id="hibiscus.navi.messages"                     name="System-Nachrichten"   icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.NachrichtList" />
      <item id="hibiscus.navi.transfer"                     name="Zahlungsverkehr"      icon-close="folder.gif" icon-open="folderopen.gif">
        <item id="hibiscus.navi.transfer.ueb"               name="Überweisungen"        icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.UeberweisungList" />
        <item id="hibiscus.navi.transfer.einzellast"        name="Lastschriften"        icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.LastschriftList" />
        <item id="hibiscus.navi.transfer.sammelueb"         name="Sammelüberweisungen"  icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.SammelUeberweisungList" />
        <item id="hibiscus.navi.transfer.sammellast"        name="Sammellastschriften"  icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.SammelLastschriftList" />
        <item id="hibiscus.navi.transfer.dauer"             name="Daueraufträge"        icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.DauerauftragList" />
      </item>
      <item id="hibiscus.navi.reporting"                    name="Auswertungen"         icon-close="folder.gif" icon-open="folderopen.gif">
        <item id="hibiscus.navi.reporting.kontoauszug"      name="Kontoauszüge"         icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.KontoauszugRpt" />
        <item id="hibiscus.navi.reporting.kategorien"       name="Umsätze nach Kategorien"    icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.UmsatzTypTree" />
        <item id="hibiscus.navi.reporting.einnahmenausgaben" name="Einnahmen/Ausgaben"  icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.EinnahmenAusgaben" />
        <item id="hibiscus.navi.reporting.sparquote"        name="Sparquote"            icon-close="page.gif"   icon-open="page.gif"        action="de.willuhn.jameica.hbci.gui.action.SparQuote" />
      </item>
    </item>
  </navigation>

  <services>
    <service name="database"    depends="" autostart="true" class="de.willuhn.jameica.hbci.rmi.HBCIDBService" />
    <service name="addressbook" depends="" autostart="true" class="de.willuhn.jameica.hbci.rmi.AddressbookService" />
  </services>

</plugin>

